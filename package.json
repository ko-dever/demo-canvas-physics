{
  "name": "playground-canvas-balls",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "clear": "npm run clear:cache & npm run clear:dist",
    "clear:cache": "rm -rf .cache/",
    "clear:dist": "rm -rf dist/",

    "lint-init": "npx eslint --init",

    "dev": "npx parcel ./src/index.html --open",

    "prebuild": "npm run clear",
    "build": "npx parcel build ./src/index.html --public-url ./ --no-cache"
  },
  "dependencies": {
    "parcel-bundler": "^1.10.1"
  },
  "devDependencies": {
    "eslint": "^5.6.1"
  }
}
